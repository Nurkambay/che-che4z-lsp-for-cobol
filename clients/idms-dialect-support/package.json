{
  "name": "idms-dialect-support",
  "displayName": "IDMS Dialect Support",
  "description": "Autocomplete, highlighting and diagnostics for IDMS dialect code and copybooks",
  "author": "Broadcom",
  "license": "EPL-2.0",
  "version": "0.0.1",
  "preview": false,
  "publisher": "BroadcomMFD",
  "engines": {
    "vscode": "^1.74.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eclipse/che-che4z-lsp-for-cobol.git"
  },
  "bugs": {
    "url": "https://github.com/eclipse/che-che4z-lsp-for-cobol/issues"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onCommand:cobol-lsp.smart-tab",
    "onCommand:cobol-lsp.dialect.register",
    "onLanguage:cobol",
    "workspaceContains:/.c4z"
],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "idms-dialect-support.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}
